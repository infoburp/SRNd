<html>
  <head>
    <title>foobar</title>
    <link type="text/css" href="/styles.css" rel="stylesheet">
  </head>
  <body class="mod">
    <div class="postform">
      <form action="modify?%%key%%" enctype="multipart/form-data" method="POST" style="text-align: center;">
        <br /><br />
        <table style="display: inline-table">
          <tr>
            <th align="right">key</th>
            <td>%%key%%</td>
          </tr>
          <tr>
            <th align="right">local name</th>
            <td><input type="text" name="local_nick" value="%%nick%%" class="posttext" /></td>
          <tr>
            <th align="right">flags</th>
            <td>%%modify_key_flagset%%</td>
          </tr>
          <tr>
            <td>&nbsp;</td>
            <td align="right"><input type="submit" value="modify" class="postbutton" /></td>
          </tr>
        </table>
      </form>
    </div>
  </body>
</html>